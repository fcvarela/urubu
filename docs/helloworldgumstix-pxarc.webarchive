}

a.jahover-active {
	color: #FFFFFF!important;
	background: url(img/havechild-active-bg.gif) no-repeat top right #BDBAA6;
}

a.mainlevel-trans {
}

a.mainlevel-trans:hover,
a.mainlevel-trans:active {
	color: #484848;
	background: url(img/tabhover-bg.gif) top #DBD9CB;
}

a.mainlevel-active-trans,
a.mainlevel-active-trans:hover,
a.mainlevel-active-trans:active {
	color: #FFFFFF!important;
	background: url(img/tabactive-bg.gif) top #BDBAA6;
}

